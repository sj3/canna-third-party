<openerp>
  <data>

    <record id="hr_expense_expense_accounting_wizard_view_form" model="ir.ui.view">
      <field name="name">Generate Accounting Entries</field>
      <field name="model">hr.expense.expense.accounting.wizard</field>
      <field name="arch" type="xml">
        <form string="Generate Accounting Entries">
          <group>
            <field name="date"/>
            <field name="period_id"/>
          </group>
          <footer>
            <button name="generate" string="Generate" type="object" class="oe_highlight"/>
            or
            <button string="Cancel" class="oe_link" special="cancel" />
          </footer>
        </form>
      </field>
    </record>

  </data>
</openerp>
