<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

  <t t-name="AccountMoveLineSearchExtension">
    <div class="ui-toolbar" style="margin-bottom:0px;">
      <div class="oe_form_char_section" name="account">
        <h4>Account :</h4>
        <input type="text" class="oe_account_select_account"/>
      </div>
      <t t-if="analytic_group_analytic_accounting">
        <div class="oe_form_char_section" name="analytic_account">
          <h4>Analytic Account :</h4>
          <input type="text" class="oe_account_select_analytic_account"/>
        </div>
      </t>
      <div class="oe_form_char_section" name="partner">
        <h4>Partner :</h4>
        <input type="text" class="oe_account_select_partner"/>
      </div>
      <div class="oe_form_dropdown_section" name="journal">
        <h4>Journal :</h4>
        <select class="oe_account_select_journal"/>
      </div>
      <div class="oe_form_char_section" name="period">
        <h4>Period :</h4>
        <input type="text" class="oe_account_select_period"/>
      </div>
      <div class="oe_form_char_section" name="reconcile">
        <h4>Reconcile :</h4>
        <input type="text" class="oe_account_select_reconcile"/>
      </div>
    </div>
  </t>

</templates>
